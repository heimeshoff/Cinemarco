{
  "name": "cinemarco",
  "version": "0.1.0",
  "description": "Personal cinema memory tracker - track movies, series, and the people you watch with",
  "type": "module",
  "scripts": {
    "dev": "concurrently --kill-others -n server,client -c cyan,green \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "cd src/Server && dotnet watch run",
    "dev:client": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "dotnet test src/Tests"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.17",
    "concurrently": "^9.2.1",
    "daisyui": "^5.5.5",
    "tailwindcss": "^4.1.17",
    "vite": "^7.0.0",
    "vite-plugin-fable": "^0.2.1"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  }
}
